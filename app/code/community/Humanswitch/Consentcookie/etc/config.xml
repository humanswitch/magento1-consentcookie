<?xml version="1.0"?>
<config>
    <modules>
        <Humanswitch_Consentcookie>
            <version>1.0.0</version>
        </Humanswitch_Consentcookie>
    </modules>
    <global>
        <models>
            <humanswitch_consentcookie>
                <class>Humanswitch_Consentcookie_Model</class>
            </humanswitch_consentcookie>
        </models>
        <blocks>
            <humanswitch_consentcookie>
                <class>Humanswitch_Consentcookie_Block</class>
            </humanswitch_consentcookie>
        </blocks>
        <helpers>
            <humanswitch_consentcookie>
                <class>Humanswitch_Consentcookie_Helper</class>
            </humanswitch_consentcookie>
        </helpers>
    </global>

    <adminhtml>
        <translate>
            <modules>
                <humanswitch_consentcookie>
                    <files>
                        <default>Humanswitch_Consentcookie.csv</default>
                    </files>
                </humanswitch_consentcookie>
            </modules>
        </translate>
        <layout>
            <updates>
                <humanswitch_consentcookie>
                    <file>humanswitch_consentcookie.xml</file>
                </humanswitch_consentcookie>
            </updates>
        </layout>
    </adminhtml>

    <frontend>
        <layout>
            <updates>
                <humanswitch_consentcookie>
                    <file>humanswitch_consentcookie.xml</file>
                </humanswitch_consentcookie>
            </updates>
        </layout>

    </frontend>
    <default>
        <consentcookie>
            <general>
                <active>0</active>
                <method>local</method>
                <cdn_url>https://cdn.humanswitch.services/cc/consentcookie/consentcookie.min.js</cdn_url>
                <override_magento_ga>0</override_magento_ga>
            </general>
            <configurator_settings>
                <active>0</active>
                <method>local</method>
            </configurator_settings>
        </consentcookie>
    </default>
</config>
